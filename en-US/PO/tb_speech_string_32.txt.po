msgctxt "e37645f8d6c51a20bfefcc2a730aa738"
msgid "ID=60282526"
msgstr ""

msgctxt "8ef8749c50d68b7925d4e64818672c4b"
msgid "(레오나의 눈망울에 눈물이 보였던 것 같다.)"
