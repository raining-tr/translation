msgctxt "0fb7fe2d2dcfa8c51fc2193c5914b341"
msgid "ID=18526001"
msgstr ""

msgctxt "5855d3cd8c239d99f1871f4213ffadd3"
msgid "ID=18526002"
msgstr ""

msgctxt "244f3db507ef3f124add59831f7ca529"
msgid "가장 기본적인 지식이네요."
msgstr ""

