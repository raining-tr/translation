msgctxt "72ecb24531dff024f51dcd5347108364"
msgid "ID=60282528"
msgstr ""

msgctxt "2d79fcbff2d3fcfc27fb1bb7588c840b"
msgid "… 꼭 부탁드려요 …"
msgstr ""

msgctxt "abc5fd3a6c16937fc02495ccaf652bc6"
msgid "ID=60282529"
msgstr ""

msgctxt "153526651038db063913894193a9cdf0"
msgid "(레오나의 딱딱한 말투가 조금은 변했다.)"
