msgctxt "00dc6505feec90ff65d33a9f8a02abdc"
msgid "ID=46028354"
msgstr ""

msgctxt "9352de0696a4dd6617861f57f8c5f49c"
msgid "(하이딩거에게 섬을 비워놓았다고 이야기했더니 하이딩거의 눈이 약간은 편안해 보인다.)"
