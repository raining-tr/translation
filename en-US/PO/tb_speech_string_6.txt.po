msgctxt "eb5f53c484131fc69304e2f0146e1bff"
msgid "ID=50282001"
msgstr ""

msgctxt "1f3e4e208b93ddbd32c2ff304d90b963"
msgid "에릭?... 에릭…\\n아, 그 모자 쓴!"
