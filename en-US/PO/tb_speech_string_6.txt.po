msgctxt "676a79e02ce5425ee6506a6dfd70439a"
msgid "ID=43101005"
msgstr ""

msgctxt "cec246d9d96329d17679a7acec3a152d"
msgid "\\n이봐, 보면 몰라? 안내고 뭐고 할 정도로 길이 복잡한가 여기가?"
