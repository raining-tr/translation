msgctxt "c7e294528d9b9f0b2819b767c8d31db8"
msgid "ID=60282028"
msgstr ""

msgctxt "9a36338c98481eba2b56db608c8b4926"
msgid "ID=60282029"
msgstr ""

msgctxt "9731ec6cb19211fabc85c5cc90c81d74"
msgid "소란스럽군.\\n어윈. 나와 얘기 좀 나눌 수 있겠나?"
