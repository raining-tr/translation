msgctxt "493590f0b5c3b3dcdacf73eb9040b95d"
msgid "ID=50122007"
msgstr ""

msgctxt "9cd29714e0d5cb0120ba50658ce06fda"
msgid "ID=50122008"
msgstr ""

msgctxt "386f80ee127f2f0908f6ee820952d6ef"
msgid "부탁 하나 해도 되겠나?"
