msgctxt "652d33a5b47fe272b6c1da053eaff3ff"
msgid "ID=50122009"
msgstr ""

msgctxt "47ba794148004717285e49c4c75d23ca"
msgid "남자 쪽의 애프터는 받지 않습니다만."
msgstr ""

msgctxt "13dbb6c1c6407a46f4b5d96b1f83ba92"
msgid "ID=50122010"
msgstr ""

msgctxt "1c16565b685dc5e41ef0365a1a48a55e"
msgid "그런 게 아니니 진지하게 들어."
