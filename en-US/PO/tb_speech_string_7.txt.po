msgctxt "60113b033b3f4401ab3174ed08fc5681"
msgid "ID=90752005"
msgstr ""

msgctxt "115ab21553d6ee4f67033666e5b1d78f"
msgid "믿고 있으라고…!\\n나만 믿고 있으라고 했는데…! 약속했는데… 난 아무것도…!!"
msgstr ""

msgctxt "8a942b9bd9d077ecb1d0128e76ef3d92"
msgid "ID=90752006"
msgstr ""

msgctxt "30f8023d7f740a5fe78c812fb224511c"
msgid "자책해봐야 아무것도 달라질 건 없어."
msgstr ""

msgctxt "97395f416a33ee894746c0ddc56fd780"
