msgctxt "32e591e61acce73c1e4c7d7348421126"
msgid "ID=30253001"
msgstr ""

msgctxt "c437b4af285c861aa49c80d0e8ad6aa0"
msgid "이렇게 된 이상, 최후의 방법을 쓰는 수 밖에 없다."
