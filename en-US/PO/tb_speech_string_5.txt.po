msgctxt "b0881426231b9ae853737770731717e0"
msgid "ID=420011502"
msgstr ""

msgctxt "3e8d90f08040738ce79c4d5add8c5a39"
msgid "(그 속도의 열차를 타고 제대로 탐사 임무를 할 수 있을 지 걱정이 든다.)"
