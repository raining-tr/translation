msgctxt "2a64f167a2e80a256f5a0e16f66f0379"
msgid "ID=10031004"
msgstr ""

msgctxt "1cd737e1c4992dc9248050083b619f89"
msgid "감사 할 필요는 없어, 이것이 우리들이 하는 일인걸."
