msgctxt "4aca9b950b7e6c1a8966c4bca414d9c7"
msgid "ID=10031006"
msgstr ""

msgctxt "a9692b129aea90d458818a9c0791a492"
msgid "네? 그건 무슨…"
msgstr ""

msgctxt "aa9c590f3fa835c8db43fb75de6786be"
msgid "ID=10031007"
msgstr ""

msgctxt "32147cf6b85daea3153e6d0565a12123"
msgid "무슨 일인지는 차차 설명해 줄게."
