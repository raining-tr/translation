msgctxt "7789a97938f4ef01c993bc2dd003f788"
msgid "ID=60166014"
msgstr ""

msgctxt "2471f6402bc94274d868fe6005326865"
msgid "알았어! 그럼 이만 가볼게!"
msgstr ""

msgctxt "431004a8a4939cd6ae0d02f47b01f756"
msgid "ID=60166015"
msgstr ""

msgctxt "a2467b9d7732cf63b54870f4ae56eaec"
msgid "아… 가기 전에 이름만이라도…!"
msgstr ""

msgctxt "07671225f242354f1f62feddf5487795"
msgid "ID=60166016"
msgstr ""

msgctxt "fd0b2c2365dc198e17069e63f85edb7e"
msgid "이름…? 이리스 인데?"
