msgstr ""

msgctxt "80eb57f7677a8449e5f3a9ce57e3a6d1"
msgid "ID=52184001"
msgstr ""

msgctxt "1f806e551d525535393e622c7f3f67e3"
msgid "알겠습니다. 그렇다면 저도 바로 가서 조사에 참가하겠습니다."
