msgctxt "18d71d2086038eb33a5581584deacbf3"
msgid "ID=48322001"
msgstr ""

msgctxt "473097f2768cc33c1db39cb303c310c7"
msgid "자, 복귀!\\n제안서 검토는 끝났어?"
