msgctxt "9fa3938651bd02b8db1afed2cda37ea8"
msgid "ID=38352001"
msgstr ""

msgctxt "fb0f88e3ed8b42445dd59cc3b5a318f6"
msgid "네네! 일단 출발하기 전에 작전 개요를 공유드리죠."
