msgstr ""

msgctxt "bc3e7343db6b7c95e16cb64d1edd3b18"
msgid "ID=66782006"
msgstr ""

msgctxt "c9287bbc6062e551aef0f115a049849f"
msgid "진 건 아닌데 졌다는 느낌이야.\\n…개운할 리가 없지."
