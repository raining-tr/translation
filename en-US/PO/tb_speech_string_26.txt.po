msgctxt "ac8b56f13b3bd58ee2724d403c5b7316"
msgid "ID=18064009"
msgstr ""

msgctxt "d2da057edd6b933a648e0955c011d6f1"
msgid "테네브리스 씨가 만질 수 있다는 것은… 유령… 비슷한 건가요?"
