msgctxt "908423afc109445390dcfbb73dbbdf7d"
msgid "ID=18115004"
msgstr ""

msgctxt "4354907043f3764f6963984b8c80a0a1"
msgid "안타깝지만 이번 기록에는 적혀있지 않을 겁니다."
