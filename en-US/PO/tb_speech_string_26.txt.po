msgctxt "2653522a8913cbf28dddff1ce537365c"
msgid "ID=88178022"
msgstr ""

msgctxt "51910e6eac2b4445542f88adff0eadfe"
msgid "아, 알았어!"
msgstr ""

msgctxt "e44c51f043b7cf8aec782a7978ee91e3"
msgid "ID=88178023"
msgstr ""

msgctxt "625c9f5c7fd633052e92e410d3d26af8"
msgid "에헤헤! 풍선이다 풍선~!"
msgstr ""

msgctxt "7012d33221d5f9a1864cf9f67b846ed2"
msgid "ID=88178024"
msgstr ""

msgctxt "b095dafe7f4d5cbecf6be64e707d8bbc"
msgid "풍선…"
