msgid "ID=38425001"
msgstr ""

msgctxt "0689078644f8c233e61209c78980076c"
msgid "ID=38425002"
msgstr ""

msgctxt "6d631a465ecf92d9f30acf7036f063f8"
msgid "ID=18435001"
msgstr ""

msgctxt "dcc33cfa12ceaad87f0a9f69708ad9ab"
msgid "저거… 브록이야! 분명 브록이야!"
