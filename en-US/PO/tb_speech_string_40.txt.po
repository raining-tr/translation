msgctxt "e2be21e8e9be8b0f7c091c78503e0309"
msgid "ID=610751015"
msgstr ""

msgctxt "9d4afae7935ebe53f92b44f155c58242"
msgid "잘 부탁해."
msgstr ""

msgctxt "723ceb23467891a2f9f5a2e557a1851b"
msgid "ID=610751016"
msgstr ""

msgctxt "2aa43b828d021687870e235b1ad02539"
msgid "그리고 그쪽의 두 얼굴은 확실히 초면이로군."
