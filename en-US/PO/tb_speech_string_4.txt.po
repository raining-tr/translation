msgctxt "5e0223e6fabe42df209ddd27b9c812b1"
msgid "ID=210065531"
msgstr ""

msgctxt "cd9d23642ad75e1d19133462c6142ad3"
msgid "죽어도 싸다고 말하고 있는 겁니까!"
