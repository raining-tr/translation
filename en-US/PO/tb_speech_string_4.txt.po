msgid "ID=260137502"
msgstr ""

msgctxt "4a626187dc2528a14ce2bcd3f29c1076"
msgid "ID=260137503"
msgstr ""

msgctxt "6e54175501fa94976f81d6e856a2f540"
msgid "흐음, 예상했던 대로…."
