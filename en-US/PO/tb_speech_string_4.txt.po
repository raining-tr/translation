msgctxt "da84ba09833f24022448d87bf42e3c40"
msgid "ID=260137501"
msgstr ""

msgctxt "203497c77c8bfe7779cf39ed86767dae"
msgid "일단, 잠시만 기다려 주시겠습니까?"
