msgctxt "19c807374c431f1cbe3e1b46fcc44e0e"
msgid "ID=17365010"
msgstr ""

msgctxt "a5d2a7a3e60cc04575e592961941e344"
msgid "웃기지마! 누구 마음대로 친구라고…!"
