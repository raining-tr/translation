msgctxt "295059594c040c1cfb4a57823bf6df45"
msgid "ID=67365012"
msgstr ""

msgctxt "0f7cd8dfa4df1fb41d43ef9464ad22bb"
msgid "응! 그러면 난 이만… 응?"
