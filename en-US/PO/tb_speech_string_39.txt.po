msgctxt "a9b5f229e12657763927abf9bb1d20f1"
msgid "ID=41031001"
msgstr ""

msgctxt "4643606e3d1a092f5f9b9026e2652c7f"
msgid "ID=41031002"
msgstr ""

msgctxt "30851ae92d032c2ed86b76921300d4d5"
msgid "무슨 일이 있었던 거야 대체. 어윈은 어디 갔어?"
