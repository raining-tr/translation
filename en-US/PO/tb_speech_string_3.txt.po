msgstr ""

msgctxt "b37d13dd2e966f7a1ed48a16d053beae"
msgid "ID=560081511"
msgstr ""

msgctxt "3404df77fbcfbea642df2fb74d49dd1e"
msgid "ID=560081512"
msgstr ""

msgctxt "6faefde7c4b59e975253b746adcb7696"
msgid "(굿보이가 군번 줄을 물어 올리더니 다시 건네주었다.)"
