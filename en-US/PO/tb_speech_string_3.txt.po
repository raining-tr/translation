msgid "ID=560081505"
msgstr ""

msgctxt "d32349c2ee168cefe05698675cdcf9cc"
msgid "왕! 왕, 왕! 왕, 왕, 왕! 아우~!"
msgstr ""

msgctxt "a7d4f94f4f0c67e926025275a56a2a27"
msgid "ID=560081506"
msgstr ""

msgctxt "223ebc2e40c0f5458e22bc501bf42109"
msgid "굿보이?"
msgstr ""

msgctxt "afe46cc40d5e57e33f54d6b3e10e5e81"
msgid "ID=560081507"
msgstr ""

msgctxt "eb0c0c9ad99ac3c2c0741fe998e1135f"
msgid "(굿보이는 군번 줄을 본인 앞발 앞에 떨어뜨려 놓더니, 한참 동안 울어댔다.)"
