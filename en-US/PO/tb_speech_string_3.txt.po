msgctxt "fa610722ec5108829a2b012088b0124d"
msgid "ID=260082002"
msgstr ""

msgctxt "3743c03f72daef3cf84371c5082b9cb8"
msgid "(굿보이가 별숲리그 본부가 있는 방향으로 짖어대기 시작했다.)"
