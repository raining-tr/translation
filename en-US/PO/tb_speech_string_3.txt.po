msgstr ""

msgctxt "606cfe0e6a5e5918dcd2d55cb4d31402"
msgid "ID=460081501"
msgstr ""

msgctxt "03d85da77bfe8a3335296c139e33f357"
msgid "(굿보이의 기세가 심상치 않다. 또 다른 무언가를 부탁하고 싶어하는 것 같다.)"
msgstr ""

