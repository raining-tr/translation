msgctxt "7696daaf676d0e9c4cdb5d0e3f9e0885"
msgid "ID=560081513"
msgstr ""

msgctxt "89dc114b5ff54aff1e663456bb4c5323"
msgid "응? 그걸 왜 다시 스승님한테?"
msgstr ""

msgctxt "4e26b91be7a01df4a5ccf0b58e625412"
msgid "ID=560081514"
msgstr ""

msgctxt "5a1140a6f16863addb2b2c7f5fc157a8"
