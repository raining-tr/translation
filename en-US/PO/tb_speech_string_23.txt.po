msgctxt "60f94048bab68131e6bbc003d97da78d"
msgid "ID=96674049"
msgstr ""

msgctxt "852c5517566fe765a81e1cd9be31fe69"
msgid "음… 아무래도 괜찮아진 것 같습니다…"
