msgctxt "98b157cdb89f2ab7037e624aaa24c75d"
msgid "ID=16674039"
msgstr ""

msgctxt "17b0eefc8c6ea9f374bc746c14672724"
msgid "웃기지마… 말뼉다구 같은 자식아…"
