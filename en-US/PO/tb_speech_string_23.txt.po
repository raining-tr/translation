msgctxt "d40ffa61f111363fd3a0beff99261a74"
msgid "ID=66695002"
msgstr ""

msgctxt "be2b48ce9f136b0a95474feb1a3363b9"
msgid "응응! 물론이야! 무슨 이야기인데?"
