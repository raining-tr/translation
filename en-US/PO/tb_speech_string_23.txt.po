msgid "ID=96674034"
msgstr ""

msgctxt "ed408bee14a3335a59429023b0fdf335"
msgid "우… 그으으…"
msgstr ""

msgctxt "6a9e0b497bca7326f36ddf5681b621bc"
msgid "ID=96674035"
msgstr ""

msgctxt "b1103e2ad9e88cb070b0d3504cf1ed94"
msgid "…그으으! 그러니까!!\\n아니…! 그럼 대체!"
