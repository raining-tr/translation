msgid "ID=10754008"
msgstr ""

msgctxt "01a108629aa5ece71e482bcc7d6598d9"
msgid "ID=10754009"
msgstr ""

msgctxt "8907e3fc2782f0c20da7710cb3ab6f97"
msgid "그저 약속해 주지 않겠나…"
