msgctxt "3ed73abb627d687da79a571b0170a4f7"
msgid "ID=10754010"
msgstr ""

msgctxt "2eaa63530ad2e8adf47e8eb14c28d1e7"
msgid "내 딸내미의 복수를… 반드시 해 주겠다고…\\n헨리와 네드 컴퍼니를… 없애 주겠다고…"
msgstr ""

msgctxt "17ed4f820c483666a6c6c3b5b8066cb5"
msgid "ID=10754011"
msgstr ""

msgctxt "36b5f944fff863f4d77f31f136470b14"
msgid "제가…"
