msgctxt "4044c36e0dab555f74417aa1733edaa7"
msgid "ID=33536015"
msgstr ""

msgctxt "fa5e99f6e6946ea7f61cd0cbfdfdcea7"
msgid "ID=33536016"
msgstr ""

msgctxt "aa4d0fb50058773ce079b6161b70a3e5"
msgid "[제 4장 : 미동하는 톱니바퀴]\\n완료."
msgstr ""

msgctxt "bb4b2744058eaf5ff3a5a3c7beb74512"
msgid "ID=33536017"
msgstr ""

msgctxt "2c1af35bad962e2c189b95789ad99d45"
msgid "ID=10034001"
msgstr ""

msgctxt "cdc50650a4ab8d92eecbeda76271d25d"
msgid "어머. 벌써 일어난 거야?\\n역시 젊으니까 좋네~"
msgstr ""

msgctxt "20113b904d9b6fdbfda04c5c2ef274d9"
msgid "ID=10034002"
msgstr ""

msgctxt "72ef05c5239141964d6d4d094a24afff"
msgid "미리엄이라고 해.\\n몸은 괜찮아? 어지럽거나 하진 않고?"
msgstr ""

msgctxt "a64ddec71895e860a29402b19d329bd9"
msgid "ID=10034003"
msgstr ""

msgctxt "2720889833cb65197837b63516d3c94f"
msgid "아… 네. 네! 멀쩡합니다!\\n걱정해 주셔서 감사합니다!"
