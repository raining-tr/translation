msgstr ""

msgctxt "a51e1baad57804c690c03477075d31d4"
msgid "ID=63526029"
msgstr ""

msgctxt "d7e66f46efd4be11202487051a0744fa"
msgid "…안녕히 가세요."
msgstr ""

msgctxt "5ba5d1fb6f66974725e3acf9d57fd4b8"
msgid "ID=43526001"
msgstr ""

msgctxt "68e180f423c19cd4ceea663eac7332af"
msgid "….결국 혼자서 멋대로 속죄한답시고 떠나가 버렸구만."
