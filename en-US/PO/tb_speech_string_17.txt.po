msgctxt "6e4ace58153a49ee000ebf9b5e59b459"
msgid "ID=60754012"
msgstr ""

msgctxt "f3c7197fdf3ee2c1808c242456eaa637"
msgid "캐서린이… 없어져 버렸어."
