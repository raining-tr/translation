msgstr ""

msgctxt "11c4bab21bb7b6f7913ebfbc386284d2"
msgid "ID=15925007"
msgstr ""

msgctxt "8769442c4bd3108b784283ccdc2f7daf"
msgid "처음 뵙겠습니다. 스텔라 씨."
