msgctxt "36f3a27c9392bc86876d0ac7281e839e"
msgid "ID=86786008"
msgstr ""

msgctxt "a3f2af1c66b804ffd36c84db5d7ba6f6"
msgid "그럴 생각은 전혀 없었지만요…"
