msgctxt "2af59440f062339159fc7568b98bd612"
msgid "ID=96676033"
msgstr ""

msgctxt "ed408bee14a3335a59429023b0fdf335"
msgid "우… 그으으…"
msgstr ""

msgctxt "190472926fb5810392649e55d7703ab5"
msgid "ID=96676034"
msgstr ""

msgctxt "b1103e2ad9e88cb070b0d3504cf1ed94"
