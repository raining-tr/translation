msgid "ID=68386014"
msgstr ""

msgctxt "701fc1a2cc7416f394aaae86d21b6a7c"
msgid "병력이 도주한 것은 내 관리 소홀이 맞아.\\n베네리스 대위님은 강하기 현장 감독도 있었으니까 내가 더 확실하게 했어야 됐어."
msgstr ""

msgctxt "04eae58f0e8b3f9e1fe4fa3f53619210"
msgid "ID=68386015"
msgstr ""

msgctxt "d8fce153b3c6c30b372dc3ab424402c1"
