msgctxt "08763ffc0af4547b190ec0aa97d9adf7"
msgid "ID=18386003"
msgstr ""

msgctxt "cb6fb852d0a0c53442998af2956d6a23"
msgid "뭐, 딱히 별일 없었던 것 같은데…"
msgstr ""

msgctxt "cf146ad943b06ea01b894d29e0413945"
msgid "ID=18386004"
msgstr ""

msgctxt "3b64b9c75a60b0a3b171b5d8a6c7f37a"
