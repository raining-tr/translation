msgctxt "8f6bd6ce894ff57ee191f81b1073cc2a"
msgid "ID=610757015"
msgstr ""

msgctxt "9d4afae7935ebe53f92b44f155c58242"
msgid "잘 부탁해."
msgstr ""

msgctxt "1d2b8beff1b4fb0ebf9279617714ba3f"
msgid "ID=610757016"
msgstr ""

msgctxt "2aa43b828d021687870e235b1ad02539"
msgid "그리고 그쪽의 두 얼굴은 확실히 초면이로군."
