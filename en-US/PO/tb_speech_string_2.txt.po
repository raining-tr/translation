msgid "ID=100385111"
msgstr ""

msgctxt "b807b2bdfe9df948ace5c27dc8ba434b"
msgid "??\\n뭐가?"
msgstr ""

msgctxt "de54b5c3f04be5428e8845d7081b85e8"
msgid "ID=100385112"
msgstr ""

msgctxt "fa95ee92533edca7aeb9cbd4ba87314a"
msgid "헨리 이사님이 얼마 전, 날 찾아왔으니까."
