msgctxt "bd0680b687d546745ca5add32524da51"
msgid "ID=210054021"
msgstr ""

msgctxt "56f54fcb8e675a3ced0ed18c40e00d4d"
msgid "그럼, 다음에 또 만나뵙기를…."
