msgctxt "b5685494b747667f2a0a2eb5b98e932c"
msgid "ID=610054010"
msgstr ""

msgctxt "b2df82ee3e2af7d93616c52140ccef08"
msgid "그나저나, 너무 말이 많은 것 아니었습니까?"
