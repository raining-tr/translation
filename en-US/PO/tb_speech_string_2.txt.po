msgctxt "6559907b7c6c55981fcc98330a6883d5"
msgid "ID=610054001"
msgstr ""

msgctxt "dd22f72182aa96df2dc38615b5296fa4"
msgid "(네브, 로이와 인사하고 물러갔다.)"
msgstr ""

msgctxt "c548451f76ffc975ef9258fe79f2cb6a"
msgid "ID=610054002"
msgstr ""

msgctxt "2c3c8175286d2d3e5a66690ad000e392"
msgid "ID=610054003"
msgstr ""

msgctxt "14eceeb1b75a24827b04b0c1771640c9"
msgid "ID=610054004"
msgstr ""

msgctxt "f099ad9a3e2750a18e0a96dd019cd353"
msgid "ID=610054005"
msgstr ""

msgctxt "92dedc3dd465e97e8a34caebcb63ec84"
msgid "…신기하네요."
msgstr ""

msgctxt "92a635903d3ebb3768632c14f94822b7"
msgid "ID=610054006"
msgstr ""

msgctxt "0f6aa6a2c0b8e1c84a5534da8d5f47a0"
msgid "…뭐가 말입니까?"
