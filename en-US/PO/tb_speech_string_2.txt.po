msgctxt "06eb01af4768ec9efb6a7c16f9e9878d"
msgid "ID=610054007"
msgstr ""

msgctxt "4d1c06a0080532da8a24879c635b2d5e"
msgid "당신, 의외로 거짓말을 안 하는군요?"
