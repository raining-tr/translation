msgctxt "e334ae7b838f8e2521367a4430db6ef6"
msgid "ID=12073009"
msgstr ""

msgctxt "2ae5fcbf5733883d9ff64f490df0916c"
msgid "글쎄요… 그건 저도 잘 모르겠군요."
