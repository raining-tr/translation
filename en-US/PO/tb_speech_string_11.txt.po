msgctxt "629af03b81fe283b30d93f4b8587ff7f"
msgid "ID=32073002"
msgstr ""

msgctxt "25ed2e7c20a764e108e2f2bb4284a000"
msgid "기분 나쁜 농담에도 정도가 있어요."
msgstr ""

msgctxt "3cca5d383472014273b9e38037175a75"
msgid "ID=32073003"
msgstr ""

msgctxt "fd3717e42601f7364289b45a0fee1dba"
msgid "아하하하, 저희 네드를 얼마나 원망하시는지 알겠네요."
