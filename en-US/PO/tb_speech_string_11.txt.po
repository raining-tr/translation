msgctxt "48627a547de160aaa6c6be59575b3211"
msgid "ID=12083010"
msgstr ""

msgctxt "0553f854e68584a6477fda731c508f1f"
msgid "철저한 보고는 중요하지, 네 뜻은 이해했어."
