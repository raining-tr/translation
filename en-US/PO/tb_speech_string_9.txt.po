msgctxt "d9ccfd9a3df682f5685f778ec14ce00b"
msgid "ID=22141001"
msgstr ""

msgctxt "fa9ef5c8d80249fed8ebf954a9273b1a"
msgid "케인을 찾아가서 기합 좀 받아봐요~"
