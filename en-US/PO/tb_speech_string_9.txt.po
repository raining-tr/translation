msgctxt "74c8379a7d71b26e54ddb3a9f53ea07e"
msgid "ID=62141045"
msgstr ""

msgctxt "04a75cf06ea2036b8eded7e5cff92663"
msgid "힘내요~ 둘 다!"
msgstr ""

msgctxt "88cdf65b3f91ed376112d72b864941c2"
msgid "ID=42141001"
msgstr ""

msgctxt "8e6350e17c85fa1fcd7d86acd20b2b65"
