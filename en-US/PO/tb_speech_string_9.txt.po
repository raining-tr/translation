msgid "ID=62141037"
msgstr ""

msgctxt "8e7d14dc953a023e3fd6a88c26126f3f"
msgid "우와. 케인~ 쿨 하네~"
msgstr ""

msgctxt "006555e2a787b73f706e8a895c2dca23"
msgid "ID=62141038"
msgstr ""

msgctxt "4bbf443b38ef64664217647e7b190897"
